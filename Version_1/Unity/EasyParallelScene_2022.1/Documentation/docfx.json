{
    "metadata": [
        {
            "src": [
                {
                    "src": "..",
                    "files": [
                        "Assets/GUPS/**/*.cs"
                    ],
                    "exclude": [
                        "Assets/Plugins/**"
                    ]
                }
            ],
            "globalNamespaceId": "GUPS.EasyParallelScene",
            "filter": "filterConfig.yml",
            "dest": "api/EasyParallelScene"
        }
    ],
    "build": {
        "globalMetadata": {
            "_appTitle": "GuardingPearSoftware documentation",
            "_appFooter": "GuardingPearSoftware documentation",
            "_enableSearch": true
        },
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            },
			{
                "src": "api",
                "files": [
                    "api.md"
                ],
                "dest": "api"
            },
            {
                "src": "api/EasyParallelScene",
                "files": [
                    "*.yml"
                ],
                "dest": "api/EasyParallelScene"
            },
			{
                "src": "manual",
                "files": [
                    "manual.md"
                ],
                "dest": "manual"
            },
            {
                "src": "manual/EasyParallelScene",
                "files": [
                    "*.md",
                    "toc.yml"
                ],
                "dest": "manual/EasyParallelScene"
            }
        ],
        "overwrite": [
            {
                "src": "..",
                "files": [
                    "Assets/**/*.md"
                ]
            }
        ],
        "resource": [		
            {
                "files": [
					"favicon.ico",
					"logo.png"
                ]
            },
            {
                "files": [
                    "resources/**/*"
                ],
                "dest": ""
            }
        ],
        "dest": "_site"
    }
}